# Meme-Coin Trading Bot Environment Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control

# =============================================================================
# RPC ENDPOINTS (Required)
# =============================================================================
# Primary Ethereum RPC endpoint
ETHRPCPRIMARY=https://mainnet.infura.io/v3/YOUR_INFURA_PROJECT_ID
# Secondary Ethereum RPC endpoint for failover
ETHRPCFALLBACK=https://eth-mainnet.alchemyapi.io/v2/YOUR_ALCHEMY_API_KEY

# Primary Solana RPC endpoint
SOLANARPCPRIMARY=https://api.mainnet-beta.solana.com
# Secondary Solana RPC endpoint for failover
SOLANARPCFALLBACK=https://solana-api.projectserum.com

# =============================================================================
# WEBSOCKET CONNECTIONS (Required)
# =============================================================================
# Primary WebSocket mempool connection
WSMEMPOOLPRIMARY=wss://mempool.space/api/v1/ws
# Secondary WebSocket connection
WSMEMPOOLFALLBACK=wss://mempool.space/api/v1/ws

# =============================================================================
# WALLET CONFIGURATION (Required)
# =============================================================================
# Cold storage address for profit sweeping
COLDSTORAGEADDRESS=0x1234567890123456789012345678901234567890
# Hot wallet address for trading
HOTWALLETADDRESS=0x0987654321098765432109876543210987654321

# =============================================================================
# NOTIFICATION SERVICES (Optional)
# =============================================================================
# Telegram bot token for notifications
NOTIFIERTOKEN=1234567890:ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890
# Telegram bot token for signal ingestion
TELEGRAMBOTTOKEN=1234567890:ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890
# Telegram chat ID for notifications
TELEGRAMCHATID=-1001234567890
# Discord webhook URL for notifications
DISCORDWEBHOOK=https://discord.com/api/webhooks/1234567890/abcdefghijklmnopqrstuvwxyz

# =============================================================================
# ML MODEL STORAGE (Required)
# =============================================================================
# URL for ML model storage and updates
MODELSTOREURL=https://models.memebot.dev/api/v1/models
# API key for model storage access
MODELSTOREAPIKEY=your_model_store_api_key_here

# =============================================================================
# BLOCKCHAIN INDEXER (Required)
# =============================================================================
# URL for blockchain indexer service
INDEXERURL=https://indexer.memebot.dev/api/v1
# API key for indexer access
INDEXERAPIKEY=your_indexer_api_key_here

# =============================================================================
# BACKUP STORAGE (Required)
# =============================================================================
# URL for backup storage service
BACKUPSTORAGEURL=https://backup.memebot.dev/api/v1
# API key for backup storage access
BACKUPSTORAGEAPIKEY=your_backup_storage_api_key_here

# =============================================================================
# GUI API CONFIGURATION (Required)
# =============================================================================
# Socket path for GUI API communication
GUIAPISOCKET=/tmp/meme_bot_gui.sock
# GUI API port (if using TCP instead of Unix socket)
GUIAPIPORT=8080
# GUI API host
GUIAPIHOST=localhost

# =============================================================================
# DATABASE CONFIGURATION (Required)
# =============================================================================
# Database connection string
DATABASE_URL=postgresql://user:password@localhost:5432/memebot
# Database connection string for SQLite (development)
DATABASE_URL_SQLITE=sqlite:///data/memebot.db

# =============================================================================
# EXCHANGE API KEYS (Required for live trading)
# =============================================================================
# Uniswap V3 API key (if using)
UNISWAP_API_KEY=your_uniswap_api_key_here
# 1inch API key (if using)
ONEINCH_API_KEY=your_1inch_api_key_here
# CoinGecko API key (if using)
COINGECKO_API_KEY=your_coingecko_api_key_here

# =============================================================================
# SECURITY CONFIGURATION (Required)
# =============================================================================
# Encryption passphrase for wallet keys (set this securely)
WALLET_PASSPHRASE=your_secure_passphrase_here
# JWT secret for API authentication
JWT_SECRET=your_jwt_secret_here
# API rate limiting key
RATE_LIMIT_KEY=your_rate_limit_key_here

# =============================================================================
# MONITORING AND ALERTING (Optional)
# =============================================================================
# Sentry DSN for error tracking
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
# Prometheus metrics endpoint
PROMETHEUS_ENDPOINT=http://localhost:9090/metrics
# Health check endpoint
HEALTH_CHECK_ENDPOINT=http://localhost:8080/health

# =============================================================================
# DEVELOPMENT CONFIGURATION (Optional)
# =============================================================================
# Debug mode (set to true for development)
DEBUG=false
# Log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO
# Enable detailed trading logs
DETAILED_TRADING_LOGS=true
# Enable performance profiling
ENABLE_PROFILING=false

# =============================================================================
# PAPER MODE CONFIGURATION (Optional)
# =============================================================================
# Enable paper mode (true for testing, false for live trading)
PAPER_MODE=true
# Paper mode starting balance (in USD)
PAPER_MODE_BALANCE=10000.0
# Paper mode starting ETH balance
PAPER_MODE_ETH_BALANCE=5.0

# =============================================================================
# RISK MANAGEMENT OVERRIDES (Optional)
# =============================================================================
# Override daily max loss percentage
OVERRIDE_DAILY_MAX_LOSS_PERCENT=
# Override profit sweep threshold
OVERRIDE_PROFIT_SWEEP_THRESHOLD=
# Override per trade percentage
OVERRIDE_PER_TRADE_PCT=
# Override max concurrent positions
OVERRIDE_MAX_CONCURRENT_POSITIONS=

# =============================================================================
# FEATURE FLAGS (Optional)
# =============================================================================
# Enable ML predictions
ENABLE_ML_PREDICTIONS=true
# Enable advanced risk management
ENABLE_ADVANCED_RISK_MANAGEMENT=true
# Enable social sentiment analysis
ENABLE_SENTIMENT_ANALYSIS=false
# Enable news analysis
ENABLE_NEWS_ANALYSIS=false
# Enable technical analysis
ENABLE_TECHNICAL_ANALYSIS=true
# Enable Solana mode
SOLANAMODE=true
# Enable Telegram mode
TELEGRAMMODE=true

# =============================================================================
# EXTERNAL SERVICE CONFIGURATION (Optional)
# =============================================================================
# Alpha Vantage API key for market data
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here
# News API key for news analysis
NEWS_API_KEY=your_news_api_key_here
# Twitter API credentials (if using sentiment analysis)
TWITTER_API_KEY=your_twitter_api_key_here
TWITTER_API_SECRET=your_twitter_api_secret_here
TWITTER_ACCESS_TOKEN=your_twitter_access_token_here
TWITTER_ACCESS_TOKEN_SECRET=your_twitter_access_token_secret_here

# =============================================================================
# INFRASTRUCTURE ENDPOINTS (Required)
# =============================================================================
# Indexer/Event service
INDEXER_URL=https://indexer.memebot.dev/api/v1
# Price feed API
PRICEFEEDAPI=https://api.coingecko.com/api/v3
# Archive/trace node
ARCHIVE_RPC=https://archive-node.memebot.dev
# Social collector endpoint
SOCIALCOLLECTORURL=https://social.memebot.dev/api/v1
# Notifier relay/Push endpoints
NOTIFIER_URL=https://notifier.memebot.dev/api/v1
# Ledger backup storage
BACKUPSTORAGEURL=https://backup.memebot.dev/api/v1
# Metrics pushgateway
METRICS_PUSHGATEWAY=https://metrics.memebot.dev
# Signing/HSM proxy for cold-sweep (optional)
SIGNINGPROXYURL=https://signing.memebot.dev/api/v1
# CI server
CISERVERURL=https://ci.memebot.dev
# Telegram bot webhook or proxy endpoint
TELEGRAMWEBHOOKURL=https://webhook.memebot.dev/telegram
# Optional external index/social APIs
EXTERNALINDEXERAPI=https://external-indexer.memebot.dev/api/v1
EXTERNALSOCIALAPI=https://external-social.memebot.dev/api/v1
# VPN/proxy endpoint
NETWORKVPNENDPOINT=https://vpn.memebot.dev
